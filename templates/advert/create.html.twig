{% extends 'base.html.twig' %}
{% block title %}createAdvert{% endblock %}

{% block body %}
    <h2>Create new advert</h2>
    <div class="row">
        <div class="col-6">
            {{ form_start(form, {'attr':{'novalidate':'novalidate'}}) }}
            {{ form_widget(form) }}
            <div class="form-group row">
                <div class="col-sm-10">
                    <input type="submit" class="btn btn-primary" value="Create">
                </div>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}